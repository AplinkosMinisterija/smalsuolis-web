    "prettier": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "prettier": "@aplinkosministerija/biip-prettier-config",
  "eslintConfig": {
    "extends": "@aplinkosministerija/eslint-config-biip-react"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,html,css}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
